#!/usr/bin/env python
# -*- coding: utf-8 -*-
"""
Sage IPython Notebook startup script.
"""
import os
from sage.repl.notebook_ipy import NOTEBOOK_DIR, SageNotebookApp

# The IPython notebook directory must exist
if not os.path.exists(NOTEBOOK_DIR):
    os.makedirs(NOTEBOOK_DIR)

app = SageNotebookApp.instance()
app.initialize()
app.start()

