    if CartanType(type) == CartanType("B3"):
        return{"A1xA1xA1": """branching_rule("B3","D2xB1","orthogonal_sum")*branching_rule("D2xB1","A1xA1xA1",[branching_rule("D2","A1xA1","isomorphic"),branching_rule("B1","A1","isomorphic")])""" \
               "A3": """branching_rule("B3","D3","extended")*branching_rule("D3","A3","isomorphic")""" \
               "G2": """branching_rule("B3","G2","miscellaneous")"""}
    if CartanType(type) == CartanType("B4"):
        return{"A1": """branching_rule("B4","A1","symmetric_power")""" \
