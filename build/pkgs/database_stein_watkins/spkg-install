#!/bin/sh
###########################################
## database_stein_watkins
###########################################

if [ -z "$SAGE_SHARE" ]; then
    echo >&2 "SAGE_SHARE undefined ... exiting"
    echo >&2 "Maybe run 'sage --sh'?"
    exit 1
fi

TARGET="${SAGE_SHARE}/stein_watkins"
if [ ! -d "${TARGET}" ]; then
   mkdir "${TARGET}"
fi

cd src

cp -R swdb "${TARGET}/"
if [ $? -ne 0 ]; then
   echo "Error installing Stein-Watkins database"
   exit 1
fi
